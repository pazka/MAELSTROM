{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dotnet: Run feed",
            "type": "shell",
            "command": "dotnet",
            "args": ["run", "--project", "feed.csproj"],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dotnet: Run corals",
            "type": "shell",
            "command": "dotnet",
            "args": ["run", "--project", "corals.csproj"],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dotnet: Run ghostNet",
            "type": "shell",
            "command": "dotnet",
            "args": ["run", "--project", "ghostNet.csproj"],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dotnet: Build All",
            "type": "shell",
            "command": "dotnet",
            "args": ["build"],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dotnet: Clean All",
            "type": "shell",
            "command": "dotnet",
            "args": ["clean"],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}
